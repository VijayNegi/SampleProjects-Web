    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch current file",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "cwd": "${workspaceFolder}/2.0-RateLimit-ExpressRateLimit",
            "localRoot": "${workspaceFolder}"
        }
        {
            "type": "node",
            "request": "attach",
            "name": "Attach Nodemon",
            "restart": true,
            "sourceMaps": false,
            "localRoot": "${workspaceFolder}"
        }
    ]
}